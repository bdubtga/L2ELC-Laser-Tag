#include <SPI.h>
#include <Wire.h>
#include "RF24.h"
#include "printf.h"
#include <Servo.h>
RF24 ReceiveRadio (9, 10);
Servo servo1;
Servo servo2;
int input = 130;


void setup() {
  Serial.begin(115200); //Start serial monitor (115200 for RF Nano)
    servo1.attach(A4);
    servo2.attach(10);
    servo1.write(130);
}

void loop() {
  while (Serial.available() > 0) {
    input = Serial.parseInt();
    servo1.write(input);
  }

}
